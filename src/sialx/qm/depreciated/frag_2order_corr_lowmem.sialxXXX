           ENDDO i 
            endif 
           ENDDO a 

           GET           t1old[a2,i1] 
           t2pp[a2,i1] = t1old[a2,i1] 

           etemp  = tpp[i1,a2]*t2pp[a2,i1] 
           print etemp 
           esum  += etemp 

         endif 
        ENDDO i1 
         endif 
