               GET Vaajj[a1,a,i,i1] 

               tppqq[a1,i1,a,i]  = Viabj[i1,a1,a,i]  
               t1ppqq[a1,i1,a,i] = Vaajj[a1,a,i,i1]  
               tppqq[a1,i1,a,i] *= 2.0  
               tppqq[a1,i1,a,i] -= t1ppqq[a1,i1,a,i] 

               taiai[a,i,b,j]    = T2old[a1,i1,b,j]*tppqq[a1,i1,a,i]   
