      PROC SET_FRAG_PARMS    
#     ------------------- 
#
#     Determine the number of fragments  
#     --------------------------------- 
      execute set_frag n_frags
     #print_scalar nfrags
      allocate wAO_frag[*] 
      allocate wOccA_frag[*] 
      allocate wOccB_frag[*] 
      allocate wVirtA_frag[*] 
      allocate wVirtB_frag[*] 

      PARDO jfrag 
         icount = 0.0 
         DO jjfrag 
            icount += 1.0 
            if jjfrag == jfrag
               exit
            endif 
         ENDDO jjfrag 

         i_aostart = icount 
         i_aoend   = 1.0  
         execute frag_index_range i_aostart i_aoend 
#
        #print_scalar i_aostart 
        #print_scalar i_aoend  
         m = 0.0  
         DO mu   
            m += 1.0  
            if m >= i_aostart 
            if m <= i_aoend 
               x1[mu] = icount 
               PUT DwAO_frag[mu] = x1[mu]  
            endif 
            endif 
         ENDDO mu   
#
      ENDPARDO jfrag 
      PARDO jfrag

         icount = 0.0 
         DO jjfrag 
            icount += 1.0 
            if jjfrag == jfrag
               exit
            endif
         ENDDO jjfrag

         i_vstart  = icount
         i_vend    = 6.0
         execute frag_index_range i_vstart i_vend
         a_vstart  = icount
         a_vend    = 7.0
         execute frag_index_range a_vstart a_vend
#
         DO i
            m = 0.0
            do i1
               m += 1.0
               if i == i1
                  exit
               endif
            enddo i1
            if m >= i_vstart
            if m <= i_vend
               xi[i] = icount
               PUT DwOccA_frag[i] = xi[i]
            endif
            endif
         ENDDO i
#
         DO a
            m = 0.0
            do a1
               m += 1.0
               if a == a1
                  exit
               endif
            enddo a1
            if m >= a_vstart
            if m <= a_vend
               xa[a] = icount
               PUT DwVirtA_frag[a] = xa[a]
            endif
            endif
         ENDDO a
      ENDPARDO jfrag

      PARDO jfrag

         icount = 0.0
         DO jjfrag
            icount += 1.0
            if jjfrag == jfrag
               exit
            endif
         ENDDO jjfrag

         i_vstart  = icount
         i_vend    = 9.0
         execute frag_index_range i_vstart i_vend
         a_vstart  = icount
         a_vend    = 10.0
         execute frag_index_range a_vstart a_vend
#
         DO j
            m = 0.0
            do j1
               m += 1.0
               if j == j1
                  exit
               endif
            enddo j1
            if m >= i_vstart
            if m <= i_vend
               xj[j] = icount
               PUT DwOccB_frag[j] = xj[j]
            endif
            endif
         ENDDO j
#
         DO b
            m = 0.0
            do b1
               m += 1.0
               if b == b1
                  exit
               endif
            enddo b1
            if m >= a_vstart
            if m <= a_vend
               xb[b] = icount
               PUT DwVirtB_frag[b] = xb[b]
            endif
            endif
         ENDDO b
      ENDPARDO jfrag
      sip_barrier
      DO mu
         GET            DwAO_frag[mu]
         wAO_frag[mu] = DwAO_frag[mu]
      ENDDO mu
      DO a
         GET              DwVirta_frag[a]
         wVirtA_frag[a] = DwVirta_frag[a]
      ENDDO a
      DO b
         GET              DwVirtb_frag[b]
         wVirtB_frag[b] = DwVirtb_frag[b]
      ENDDO b
      DO i
         GET             DwOcca_frag[i]
         wOccA_frag[i] = DwOcca_frag[i]
      ENDDO i
      DO j
         GET             DwOccb_frag[j]
         wOccB_frag[j] = DwOccb_frag[j]
      ENDDO j

#
      ENDPROC SET_FRAG_PARMS    
