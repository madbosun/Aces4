            if s == icount
               GET wC[a,i] 
               GET x1new[a,i] 
               DO j
                  xj[j]      = wOccA_frag[j]
                  execute return_sval xj[j]  m
                  if m == jcount
                     GET SOVL[j,a] 
                     GET SOVL[i,j] 

                     tpp[i,a] = SOVL[i,j]*SOVL[j,a] 
                     etemp = wC[a,i]*tpp[i,a] 
                     etemp*=  2.0  
                     esum += etemp 
                     etemp = x1new[a,i]*tpp[i,a] # VFL TEST  
                     etemp*= -2.0  
                     esum += etemp 

                  endif # m == jcount
               ENDDO j
            endif # s == icount
         ENDDO i
            endif 
         ENDDO a
#
         DO a
            xa[a]      = wVirtA_frag[a]
