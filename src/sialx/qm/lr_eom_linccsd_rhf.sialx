normcrit = 0.0000000000001
ZERO = 0.0
one = 1.0
root = 0
subspc = eom_subspc
roots = eom_roots

is_ccpt = 0.0

# Initilize the vaiable to zmax and imax (memory of ERD and OED).
zmax = 0.0
imax = 0.0
execute compute_int_scratchmem oed_ovl zmax imax

print " "
#print "-- Parallel left-right EE-EOM-linCC program"
print "-- Parallel right EE-EOM-linCC program"
print "-- J. Comp. Chem. 143, 164103 (2015)"
