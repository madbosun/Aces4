             PUT Fpqb_prev[mu,nu] = Fpq_b[mu,nu]

             PUT DHFa_Prev[mu,nu] = DHFa[mu,nu]
             PUT DHFb_Prev[mu,nu] = DHFb[mu,nu]

       ENDPARDO mu, nu

       DO mu
