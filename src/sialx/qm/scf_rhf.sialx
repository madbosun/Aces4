     PROC LEVEL_SHIFTAB
#    ------------------

     DO P
     DO p1

        IF p1 >= naocc
           IF p >= naocc
              Fpqa_MO_VSHIFT[p,p1] +=  Shifta
           ENDIF
        ENDIF

     ENDDO p1
     ENDDO P

     DO q
     DO q1

        IF q1 >= nbocc
           IF q >= nbocc
              Fpqb_MO_VSHIFT[q,q1] +=  Shiftb
           ENDIF
        ENDIF

     ENDDO q1
     ENDDO q

     ENDPROC LEVEL_SHIFTAB
