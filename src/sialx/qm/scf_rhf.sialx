       PROC FOCK_DIAG  
#      -------------- 
#
#      First put the transposed Fock arrays into static arrays 
#      ------------------------------------------------------- 
#
       DO mu   
       DO nu  
#
          GET             FTpq_a[mu,nu] 
          Txx[mu,nu]    = FTpq_a[mu,nu]
#
          FSpq_a[mu,nu] =  Txx[mu,nu]
#
       ENDDO nu  
       ENDDO mu   

       execute eigen_calc FSpq_a ca

       ENDPROC FOCK_DIAG 
