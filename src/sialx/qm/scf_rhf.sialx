       PROC C_BTRAN 
#      ------------ 
#
#      Transform the coefficient array back to the original basis 
#      ---------------------------------------------------------- 
#
       PARDO mu, p  
#
          Txp[mu,p] = 0.0 
#
          DO nu 
#
             GET          Qxx[mu,nu] 
             Txx[mu,nu] = Qxx[mu,nu]
#
             T1xp[mu,p] = Txx[mu,nu]*ca[nu,p]   
             Txp[mu,p] += T1xp[mu,p] 
#
          ENDDO nu 
#
          PUT CBT_a[mu,p] = Txp[mu,p] 
#
       ENDPARDO mu, p  
#
       PARDO mu, q
# 
          Txq[mu,q] = 0.0 
#
          DO nu 
#
             GET          Qxx[mu,nu] 
             TXX[mu,nu] =  Qxx[mu,nu]
#
             T1xq[mu,q] = Txx[mu,nu]*ca[nu,q]   
             Txq[mu,q] += T1xq[mu,q] 
#
          ENDDO nu 
#
          PUT CBT_b[mu,q] = Txq[mu,q] 
#
       ENDPARDO mu, q
       sip_barrier 
#
#      Put the transformed coefficients into static arrays 
#      --------------------------------------------------- 
#
       DO mu 
       DO p 
          GET        CBT_a[mu,p] 
          Txp[mu,p] = CBT_a[mu,p]
          ca[mu,p]  = Txp[mu,p]
       ENDDO p 
       ENDDO mu  

       sip_barrier 
#
       ENDPROC C_BTRAN 
