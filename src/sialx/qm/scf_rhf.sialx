     PROC BUILT_AO_FOCK
#    ---------------------
#
       PARDO mu, nu
             GET Fpq_a[mu,nu]
             Txx[mu,nu] = 0.0
             PUT Fpq_a[mu,nu] = Txx[mu,nu]
       ENDPARDO mu,nu
       PARDO mu, nu
             GET Fpq_b[mu,nu]
             Txx[mu,nu] = 0.0
             PUT Fpq_b[mu,nu] = Txx[mu,nu]
       ENDPARDO mu,nu

       CREATE SCa
       CREATE SCb

       sip_barrier

       PARDO mu, nu
             DO p
                Txp[mu,p]      = OVLP[mu,nu] * Ca[nu,p]
                PUT SCa[mu,p] += Txp[mu,p]
             ENDDO p
       ENDPARDO mu, nu

       PARDO mu, nu
             DO q
                Txq[mu,q]      = OVLP[mu,nu] * Ca[nu,q]
                PUT SCb[mu,q] += Txq[mu,q]
             ENDDO q
       ENDPARDO mu, nu
       sip_barrier

       PARDO p, p1
#            GET Fpqa_MO(p,p1)
            DO mu

               GET SCa[mu,p]
               Txp[mu,p]   = SCa[mu,p]
               Ixp[mu,p1]  = Fpqa_MO_VSHIFT[p,p1] * Txp[mu,p]

               DO nu
                  GET SCa[nu,p1]
                  Txp[nu,p1]   = SCa[nu,p1]
                  Ixx[mu,nu]   = Ixp[mu,p1] * Txp[nu,p1]

                  PUT Fpq_a[mu,nu] += Ixx[mu,nu]
               ENDDO nu
            ENDDO mu
      ENDPARDO p, p1

      PARDO q, q1
#            GET Fpqb_MO(q,q1)
            DO mu
               GET SCb[mu,q]
               Txq[mu,q]   = SCb[mu,q]
               Jxq[mu,q1]  = Fpqb_MO_VSHIFT[q,q1] * Txq[mu,q]

               DO nu

                  GET SCb[nu,q1]
                  Txq[nu,q1]   = SCb[nu,q1]
                  Ixx[mu,nu]   = Jxq[mu,q1] * Txq[nu,q1]

                  PUT Fpq_b[mu,nu] += Ixx[mu,nu]
               ENDDO nu
            ENDDO mu
      ENDPARDO q, q1

      sip_barrier
##bgn_debug
