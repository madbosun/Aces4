endpardo a,i,a1,i1
server_barrier
     PARDO  a, a1, i, i1
	  REQUEST            VCHACT2AA[a,i,a1,i1]
	  Taiai[a,i,a1,i1] = VCHACT2AA[a,i,a1,i1]
#
	  R3aiai[a1,i,a,i1] = 0.0
	  Raiai[a,i,a1,i1]  = 0.0
#
	  if a == a1
	     Raiai[a,i,a1,i1]   = Taiai[a,i,a1,i1]
	     execute anti_symm_v Raiai[a,i,a1,i1]
	  endif
#
	  if a < a1
	     Raiai[a,i,a1,i1]   = Taiai[a,i,a1,i1]
	     R3aiai[a1,i,a,i1]  = Taiai[a,i,a1,i1]
	     R3aiai[a1,i,a,i1] *= -1.0
	  endif
#
	  put DRNaiai[a,i,a1,i1] += Raiai[a,i,a1,i1]
	  put DRNaiai[a1,i,a,i1] += R3aiai[a1,i,a,i1]
     ENDPARDO a, a1, i, i1
server_barrier
     PARDO a, a1, i, i1
#
           get DRNaiai[a,i,a1,i1]
           Taiai[a,i,a1,i1] = DRNaiai[a,i,a1,i1]
#
           R2aiai[a,i1,a1,i]=0.0
           Raiai[a,i,a1,i1]=0.0
#
           if i == i1
#
              Raiai[a,i,a1,i1]=Taiai[a,i,a1,i1]
              execute anti_symm_o Raiai[a,i,a1,i1]
#
           endif
#
           if i < i1
#
              Raiai[a,i,a1,i1]=Taiai[a,i,a1,i1]
              R2aiai[a,i1,a1,i]=Taiai[a,i,a1,i1]
              R2aiai[a,i1,a1,i]*=-1.0
#
           endif

#
           put D2RNaiai[a,i,a1,i1]+=Raiai[a,i,a1,i1]
           put D2RNaiai[a,i1,a1,i]+=R2aiai[a,i1,a1,i]
#
     ENDPARDO  a, a1, i, i1
server_barrier
pardo a,i,a1,i1
    get D2RNaiai[a,i,a1,i1]
    Taiai[a,i,a1,i1] = D2RNaiai[a,i,a1,i1]
    prepare VCHACT2AA[a,i,a1,i1] = Taiai[a,i,a1,i1]
endpardo a,i,a1,i1
server_barrier
delete DRNaiai
delete D2RNaiai
server_barrier

sum1 = 0.0
pardo a,i,a1,i1
    request VCHACT2AA[a,i,a1,i1]
    request VCHACT2AA[a,i1,a1,i]
    Taiai[a,i,a1,i1]  = VCHACT2AA[a,i,a1,i1]
    Taiai[a,i,a1,i1] += VCHACT2AA[a,i1,a1,i]
    T1aiai[a,i,a1,i1] = Taiai[a,i,a1,i1]

    etemp = Taiai[a,i,a1,i1]*T1aiai[a,i,a1,i1]
    sum1 += etemp
endpardo a,i,a1,i1
server_barrier
sum2 = 0.0
collective sum2 += sum1
print sum2
ENDPROC SYMMETRHC_A

PROC place_HB
server_barrier
Tk[ksub] = 1.0
pardo a,i
    get VCHACT1A[a,i]
    Tkai[ksub,a,i] = Tk[ksub]*VCHACT1A[a,i]
    prepare HB1_a[ksub,a,i] = Tkai[ksub,a,i]
endpardo a,i

pardo a,i,a1,i1
    request VCHACT2AA[a,i,a1,i1]
    Tkaiai[ksub,a,i,a1,i1] = Tk[ksub]*VCHACT2AA[a,i,a1,i1]
    prepare HB2_aa[ksub,a,i,a1,i1] = Tkaiai[ksub,a,i,a1,i1]
endpardo a,i,a1,i1

pardo a,i,b,j
    request VCHACT2AB[a,i,b,j]
    Tkaibj[ksub,a,i,b,j] = Tk[ksub]*VCHACT2AB[a,i,b,j]
    prepare HB2_ab[ksub,a,i,b,j] = Tkaibj[ksub,a,i,b,j]
endpardo a,i,b,j
server_barrier
ENDPROC place_HB

